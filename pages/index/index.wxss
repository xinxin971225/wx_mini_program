/**index.wxss**/
.change {
  color: #8995a1;
  font-size: 24rpx;
  padding: 20rpx 20rpx;
}

.navigator-hover {
  color: #999999;
}

.input .weui-cell {
  display: flex;
  flex-wrap: nowrap;
  border: 3px solid #666666;
  border-bottom: 1px solid #c7cee0;
}

.input .weui-input {
  width: 94%;
  height: 360rpx;
  margin: 0;
  box-sizing: border-box;
  background-color: #fff;
  padding: 30rpx 0 30rpx 30rpx;
}

.input .icon-small-wrp {
  width: 6%;
}

.input .weui-cells__title {
  height: 200rpx;
  padding: 20rpx 0;
}

.input .title {
  font-size: 28rpx;
  color: #8995a1;
}